# MSP Platform environment example
# Copy to .env and adjust values for local development

# Database (Postgres for AWX)
POSTGRES_PASSWORD=changeme

# Redis
REDIS_PASSWORD=changeme

# AWX (Ansible Controller)
AWX_SECRET_KEY=changeme
AWX_ADMIN_USER=admin
AWX_ADMIN_PASSWORD=changeme

# Vault
VAULT_ROOT_TOKEN=changeme

# Compliance service
CLIENT_ID=default
COMPLIANCE_LEVEL=level2

# Optional
# COMPOSE_PROJECT_NAME=msp-platform
# ANSIBLE_STDOUT_CALLBACK=yaml
# ANSIBLE_HOST_KEY_CHECKING=false

